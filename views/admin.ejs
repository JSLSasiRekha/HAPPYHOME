        let set=document.getElementById('setting');
        set.addEventListener("click",()=>
        { if(document.getElementById('management').style.display=='flex'){
            document.getElementById('management').style.display='none'
        }
        else{
            document.getElementById('management').style.display='flex'
        }
            
        })
        document.getElementsByClassName("tablink")[0].click();
       

        function openCity(evt, cityName) {
          var i, x, tablinks;
          x = document.getElementsByClassName("city");
          for (i = 0; i < x.length; i++) {
            x[i].style.display = "none";
          }
          tablinks = document.getElementsByClassName("tablink");
          for (i = 0; i < x.length; i++) {
            tablinks[i].classList.remove("w3-light-grey");
          }
          document.getElementById(cityName).style.display = "block";
          evt.currentTarget.classList.add("w3-light-grey");
        }
        document.getElementsByClassName("talink")[0].click();

function openity(evt, cityName) {
  var i, x, tablinks;
  x = document.getElementsByClassName("cty");
  for (i = 0; i < x.length; i++) {
    x[i].style.display = "none";
  }
  tablinks = document.getElementsByClassName("talink");
  for (i = 0; i < x.length; i++) {
    tablinks[i].classList.remove("w3-light-grey");
  }
  document.getElementById(cityName).style.display = "block";
  evt.currentTarget.classList.add("w3-light-grey");
}
        
